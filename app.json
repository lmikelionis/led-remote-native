{
  "expo": {
    "name": "LedRemote",
    "description": "Led Remote for personal, home automation project",
    "privacy": "unlisted",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.9.70",
    "orientation": "portrait",
    "ios": {
      "bundleIdentifier": "com.lukas.ledremote",
      "supportsTablet": false
    },
    "android": {
      "package": "com.lukas.ledremote"
    },
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "slug": "ledremote",
    "githubUrl": "https://github.com/lmikelionis/led-remote-native"
  }
}